    "karma": "1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^3.0.4",
    "mocha": "^3.4.2",
    "ts-node": "^3.2.0",
    "tslib": "1.5.0",
    "tslint": "4.3.1",
    "tslint-eslint-rules": "3.2.3",
